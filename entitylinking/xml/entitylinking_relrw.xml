<?xml version="1.0" encoding="UTF-8"?>
<EL>
	<paramenter>
		<!-- 数据集文件夹路径 -->
		<fileDirPath>./data/ace2004/test</fileDirPath>
		<!-- 上下文窗口 -->
		<contextWindow>500</contextWindow>
		<!-- 随机游走alpha -->
		<alpha>0.85</alpha>
		<!-- 收敛精度 -->
		<convergencePrecise>0.01</convergencePrecise>
		<!-- 散度gamma值 -->
		<gamma>20</gamma>
		<!-- 限制实体正文字符长度 -->
		<entityContentLen>1000</entityContentLen>
		<!-- 候选实体剪枝阈值 -->
		<candidateEntityNumThresh>40</candidateEntityNumThresh>
		<!-- 流行度阈值 -->
		<popularityThresh>400</popularityThresh>
		<!-- 实体共现次数阈值 -->
		<cooccurenceThresh>5</cooccurenceThresh>
		<!-- 语义相似性权值 -->
		<semanticSimWeight>0.3</semanticSimWeight>
		<!-- 上下文相似性权值 -->
		<contextSimWeight>0.3</contextSimWeight>
		<!-- 先验权值 -->
		<popularityWeight>0.4</popularityWeight>
		<!-- 空值阈值 -->
		<nilThres>0.2</nilThres>
	</paramenter>
	<constant>
		<!-- 空实体链接 -->
		<nil>nil</nil>
	</constant>
	<indexFields>
		<!-- 实体共现关系域 -->
		<entityRelationFields>
			<field1>entityRelationKey</field1>
			<field2>entityCount</field2>
			<field3>entityRelationValue</field3>
		</entityRelationFields>
	</indexFields>
</EL>