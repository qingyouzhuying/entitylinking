<?xml version="1.0" encoding="UTF-8"?>
<EL>
	<paramenter>
		<!-- 数据集文件夹路径 -->
		<fileDirPath>./data/ace2004/RawTexts</fileDirPath>
		<!-- 上下文窗口 -->
		<contextWindow>10</contextWindow>
		<!-- 随机游走alpha -->
		<alpha>0.85</alpha>
		<!-- 收敛精度 -->
		<convergencePrecise>0.01</convergencePrecise>
		<!-- 散度gamma值 -->
		<gamma>20</gamma>
	</paramenter>
	<indexFields>
		<!-- 实体共现关系域 -->
		<entityRelationFields>
			<field1>entityRelationKey</field1>
			<field2>entityCount</field2>
			<field3>entityRelationValue</field3>
		</entityRelationFields>
	</indexFields>
</EL>